import "resources/case_studies/Insurance/Money.obs"

main resource contract Bank {
    Money@Owned mon;
    // the bank's private key for making tokens
    int key;

    Bank@Owned() {
        // TODO: generate key
        mon = new Money(100);
        key = 35;
    }

    transaction requestMoney(int amount) returns Money@Owned{
         return mon.getAmountOfMoney(amount);
    }

}


