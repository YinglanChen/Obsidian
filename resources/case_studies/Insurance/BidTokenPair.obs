import "resources/case_studies/Insurance/Token.obs"
import "resources/case_studies/Insurance/Bid.obs"

main resource contract BidTokenPair {
    Bid@Unpurchased bid;
    // TODO: Token or Tokens?
    Token@Owned token;

    BidTokenPair@Owned(Bid@Unpurchased b, Token@Owned t) {
        bid = b;
        token = t;
    }

    transaction getBid() returns Bid@Unpurchased {
        return bid;
    }

    transaction getToken() returns Token@Owned {
        return token;
    }


}