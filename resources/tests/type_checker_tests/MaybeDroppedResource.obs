resource contract Money {}

resource contract Wallet {
	 owned Wallet() {
	 	  ->Empty;
	 }

	 state Full {
	       owned Money m;
	 }

	 state Empty {
	 }

	 transaction loseMoney() available in Full, Empty {
	       // Error -- maybe losing Money!
	       ->Empty;
	 }

	 transaction loseMoney(owned Money m) {
	       <-m;
	 }
}