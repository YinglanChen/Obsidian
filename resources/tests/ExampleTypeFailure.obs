unique contract C {
    C(string s) {
        ->HasString({s = s});
    }

    state HasString {
        string s;
    }
}

main contract IntContainer {
    int x;
    string s;
    bool b;

    transaction setX(int newX) returns int {
        int oldX = this.x;
        this.x = newX + true;
        createC();
        return this.b;
    }

    transaction createC(string s) returns C {
        return new C(s);
    }
}