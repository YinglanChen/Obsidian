main contract States {
    int y;

    state S1 {
        int x;
        transaction s1_only() {
            x = 1;
        }
    }

    state S2 {
        transaction s2_only() {
            y = 1;
        }
    }

    transaction all_states() {
        throw;
    }

    States() {
        ->S1({x = y});
    }
}