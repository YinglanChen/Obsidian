main contract SimpleVerification {
    int x;
    int y;

    SimpleVerification() {
        x = 0;
        y = 1;
    }

    state S1
    requires x > 0;
    {
        int s1;
    }

    state S2 {
        int s2;
    }

    transaction doStuff(int a, bool b)
        ensures x > 0;
    {
        x = 1;
        other();
        ->S1({s1 = a});
    }

    transaction other()
        requires x > 0;
        ensures x > 0;
    {

    }
}