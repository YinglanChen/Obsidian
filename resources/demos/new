contract Player {
    state ToChoose;
    state Rock;

    Player() {
        ->ToChoose;
    }

    transaction rock() {
        ->Rock;
    }
}

main contract RockPaperScissors {
    transaction test() returns string {
        Player p = new Player();
        p.rock();

        if (p in Rock) {
            return "rock";
        } else {
            return "blah";
        }
    }
}

